
fun curry(f) {
	/* TODO(tyler): '_f' is currently not
	 * allowed as an identifier as the scanner
	 * doesn't pick up the preceding '_'. */
	fun f_(arg) {
		f(arg);
		return null;
	}
	return f_;
}

/* TODO(tyler): a function with no parameters 
 * crashes when called. */
fun main(a) {
	var my_print = curry(print);
	my_print("Hello, World");
	var a = List(1,2,3);
	print(a);

	var b = head(a);
	print(b);



}

main(1);
